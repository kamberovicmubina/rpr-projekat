<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/style.css" />
    </stylesheets>
    <children>
      <TableView fx:id="contractTableView" prefHeight="200.0" prefWidth="200.0" styleClass="table-row-cell">
        <columns>
          <TableColumn fx:id="Title" prefWidth="150.0" text="%title" />
          <TableColumn fx:id="DateFrom" prefWidth="150.0" text="%dateFrom" />
            <TableColumn fx:id="DateTo" prefWidth="150.0" text="%dateTo" />
            <TableColumn fx:id="Value" prefWidth="150.0" text="%value" />
        </columns>
      </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addContractBtn" mnemonicParsing="false" onAction="#onAddContract" styleClass="buttonClass" text="%addContract" />
            <Button fx:id="deleteContractBtn" mnemonicParsing="false" onAction="#onDeleteContract" styleClass="buttonClass" text="%deleteContract" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
