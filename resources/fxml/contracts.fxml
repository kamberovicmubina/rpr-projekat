<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="contractTableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="Title" prefWidth="232.0" text="%title" />
          <TableColumn fx:id="DateFrom" prefWidth="185.0" text="%dateFrom" />
            <TableColumn fx:id="DateTo" prefWidth="182.0" text="%dateTo" />
        </columns>
      </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addContractBtn" mnemonicParsing="false" onAction="#onAddContract" text="%addContract" />
            <Button fx:id="deleteContractBtn" mnemonicParsing="false" onAction="#onDeleteContract" text="%deleteContract" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
